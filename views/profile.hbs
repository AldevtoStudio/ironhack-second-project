<div>
  <img id="profile-img" src="{{profile.picture}}" alt="{{profile.name}}" />

  <h2>{{profile.name}}</h2>

  {{#if userProfile}}
    <a href="/profile/edit">Edit profile</a>
  {{/if}}
</div>

{{#each cards}}
  {{#if media}}
    <img src="{{media}}" alt="" />
  {{/if}}

  <p>{{text}}</p>

  <p>{{title}}</p>
{{/each}}

<p>Total scores of your cards</p>

{{#each cards}}

  <p>{{totalScore}}</p>

{{/each}}